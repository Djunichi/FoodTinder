{
  "family": "foodtinder-task",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "foodtinder-app",
      "image": "470727823954.dkr.ecr.us-east-1.amazonaws.com/foodtinder-app:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 9000,
          "hostPort": 9000
        }
      ],
      "environment": [
        { "name": "MONGO_URI", "value": "mongodb://localhost:27017/foodtinder" },
        { "name": "DATABASE_URL", "value": "postgres://admin:admin@localhost:5432/foodtinder?sslmode=disable" }
      ],
      "memory": 256
    }
  ]
}